@article{SCHMID2007407,
title = "Multivariate extensions of Spearman's rho and related statistics",
journal = "Statistics \& Probability Letters",
volume = "77",
number = "4",
pages = "407 - 416",
year = "2007",
issn = "0167-7152",
doi = "https://doi.org/10.1016/j.spl.2006.08.007",
url = "http://www.sciencedirect.com/science/article/pii/S0167715206002586",
author = "Friedrich Schmid and Rafael Schmidt",
keywords = "Spearman's rho, Multivariate measure of association, Copula, Nonparametric estimation, Empirical copula, Weak convergence, Asymptotic variance, Nonparametric bootstrap",
abstract = "Multivariate measures of association are considered which, in the bivariate case, coincide with the population version of Spearman's rho. For these measures, nonparametric estimators are introduced via the empirical copula. Their asymptotic normality is established under rather weak assumptions concerning the copula. The asymptotic variances are explicitly calculated for some copulas of simple structure. For general copulas, a nonparametric bootstrap is established."
}

